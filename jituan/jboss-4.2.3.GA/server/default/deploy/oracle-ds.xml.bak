    <driver-class>oracle.jdbc.driver.OracleDriver</driver-class>
      <user-name>baf</user-name>
      <password>password</password>
	<prefill>true</prefill>
    <min-pool-size>2</min-pool-size>
    <max-pool-size>5</max-pool-size>
